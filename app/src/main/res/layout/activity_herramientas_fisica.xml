<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".herramientas_fisica">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <Space
                android:layout_width="15dp"
                android:layout_height="20dp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="296dp"
                android:layout_height="72dp"
                android:layout_gravity="center"
                android:fontFamily="@font/alata"
                android:text="@string/txt_btnPantallaHerramientas_tablasFisica_titulo"
                android:textAlignment="center"
                android:textColor="#040404"
                android:textSize="25dp" />


            <ImageView
                android:id="@+id/f1"
                android:layout_width="335dp"
                android:layout_height="440dp"
                android:layout_gravity="center"
                android:background="@drawable/htas_fisica_01"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="112dp" />
            <ImageView
                android:id="@+id/f2"
                android:layout_width="358dp"
                android:layout_height="380dp"
                android:layout_gravity="center"
                android:background="@drawable/htas_fisica_02"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="112dp" />
            <ImageView
                android:id="@+id/f3"
                android:layout_width="358dp"
                android:layout_height="380dp"
                android:layout_gravity="center"
                android:background="@drawable/htas_fisica_03"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="112dp" />
            <ImageView
                android:id="@+id/f4"
                android:layout_width="358dp"
                android:layout_height="380dp"
                android:layout_gravity="center"
                android:background="@drawable/htas_fisica_04"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="112dp" />
            <ImageView
                android:id="@+id/f5"
                android:layout_width="358dp"
                android:layout_height="380dp"
                android:layout_gravity="center"
                android:background="@drawable/htas_fisica_05"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="97dp"
                tools:layout_editor_absoluteY="112dp" />

        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>